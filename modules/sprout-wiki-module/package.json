{
  "name": "@savantly/sprout-wiki-plugin",
  "version": "1.1.0",
  "description": "Wiki plugin for the Sprout platform",
  "main": "src/main/resources/index.ts",
  "homepage": "https://github.com/savantly-net/sprout-platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/savantly-net/sprout-platform.git"
  },
  "scripts": {
    "build": "rimraf dist aot && npm run transpile && npm run package",
    "ci": "npm run e2e && npm run test",
    "clean": "rimraf node_modules doc dist aot && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "docs": "typedoc --options typedoc.json src/main/resources/index.ts",
    "lint": "tslint --project tsconfig.json --force \"src/**/*.ts\"",
    "package": "rollup -c",
    "postinstall": "npm run update-webdriver",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "pretest": "rimraf aot dist && npm run lint",
    "publish": "npm publish --prefix ./dist/",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "transpile": "ngc -p tsconfig.json",
    "update-webdriver": "webdriver-manager update",
    "watch": "webpack --watch --progress --profile",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
  "keywords": [
    "sprout",
    "savantly",
    "angular"
  ],
  "author": "Jeremy@Savantly.net",
  "license": "MIT",
  "peerDependencies": {
    "@angular/common": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@savantly/ngx-sprout-plugin": "^1.1.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/common": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@savantly/ngx-sprout-plugin": "^1.1.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.17",
    "@angular/compiler": "^4.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.28",
    "@types/selenium-webdriver": "^3.0.7",
    "codelyzer": "^3.2.0",
    "core-js": "^2.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^5.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.10",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-auto-external": "^1.0.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve-angular": "^2.0.3",
    "rollup-plugin-pkg-generator": "^1.1.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "rollup-plugin-uglify": "^2.0.1",
    "systemjs": "^0.20.19",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.7.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.1.1",
    "uglify-js": "^3.1.1",
    "webdriver-manager": "^12.0.6"
  }
}
