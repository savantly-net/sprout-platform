{
    "author": "Jeremy Branham <Jeremy@savantly.net>",
    "license": "Apache-2.0",
    "private": true,
    "name": "@savantly/sprout",
    "version": "1.0.0",
    "repository": "github:savantly/sprout-platform",
    "scripts": {
      "setup-dev": "yarn && yarn bootstrap && yarn link-all && yarn build",
      "bootstrap": "lerna bootstrap",
      "build": "yarn packages:build",
      "jest": "jest --notify --watch",
      "lint": "eslint . --ext .js,.tsx,.ts --cache",
      "jest-ci": "mkdir -p reports/junit && export JEST_JUNIT_OUTPUT_DIR=reports/junit && jest --ci --reporters=default --reporters=jest-junit -w ${TEST_MAX_WORKERS:-100%}",
      "link-all": "lerna exec --parallel yarn link",
      "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
      "lint:fix": "yarn lint --fix",
      "packages:build": "lerna run clean && lerna run build",
      "packages:prepare": "lerna version --no-push --no-git-tag-version --force-publish --exact",
      "packages:publish": "lerna publish from-package --contents dist",
      "packages:publishCanary": "lerna publish from-package --contents dist --dist-tag canary --yes",
      "packages:publishLatest": "lerna publish from-package --contents dist --yes",
      "packages:publishNext": "lerna publish from-package --contents dist --dist-tag next --yes",
      "packages:typecheck": "lerna run typecheck",
      "precommit": "sprout-toolkit precommit",
      "prettier:check": "prettier --list-different \"**/*.{ts,tsx,scss}\"",
      "prettier:write": "prettier --list-different \"**/*.{ts,tsx,scss}\" --write",
      "typecheck": "tsc --noEmit"
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged && npm run precommit"
      }
    },
    "lint-staged": {
      "*.{js,ts,tsx}": [
        "eslint --ext .js,.tsx,.ts --cache --fix"
      ],
      "*.{json,scss}": [
        "prettier --write"
      ]
    },
    "devDependencies": {
      "eslint": "7.4.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-jsdoc": "28.6.1",
      "eslint-plugin-prettier": "3.1.4",
      "eslint-plugin-react-hooks": "4.0.5",
      "husky": "4.2.1",
      "jest": "26.0.0",
      "lerna": "^3.20.2",
      "lint-staged": "10.0.7",
      "mocha": "7.0.1",
      "module-alias": "2.2.2",
      "prettier": "1.19.1",
      "typescript": "3.9.3"
    },
    "dependencies": {
    },
    "resolutions": {
      "caniuse-db": "1.0.30000772"
    },
    "workspaces": {
      "packages": [
        "packages/*"
      ],
      "nohoist": [
        "**/@types/*",
        "**/@types/*/**"
      ]
    },
    "_moduleAliases": {
      "puppeteer": "node_modules/puppeteer-core"
    },
    "engines": {
      "node": ">=12 <13"
    },
    "volta": {
      "node": "12.18.4"
    }
  }